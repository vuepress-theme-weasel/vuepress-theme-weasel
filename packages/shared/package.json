{
  "name": "@mr-huang/vuepress-shared",
  "version": "1.0.0-beta.1",
  "description": "weasel theme",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "author": "mr-huang",
  "scripts": {
    "build": "cpx \"src/**/*.{d.ts,vue,scss}\" lib && tsc --build tsconfig.build.json",
    "clean": "rimraf lib *.tsbuildinfo",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "cpx \"src/**/*.{d.ts,vue,scss}\" lib -w",
    "dev:ts": "tsc --build -w tsconfig.build.json"
  },
  "homepage": "https://cavinHuang.github.io/vuepress-theme-weasel/packages/shared",
  "repository": {
    "type": "git",
    "url": "https://github.com/cavinHuang/vuepress-theme-weasel"
  },
  "bugs": {
    "url": "https://github.com/cavinHuang/vuepress-theme-weasel/issues"
  },
  "keywords": [
    "vue3",
    "weasel",
    "vuepress plugin",
    "vuepress"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "chalk": "4.1.2",
    "ora": "5.4.1"
  },
  "dependencies": {
    "@vuepress/core": "2.0.0-beta.38",
    "dayjs": "^1.11.1",
    "execa": "5.1.1"
  }
}
